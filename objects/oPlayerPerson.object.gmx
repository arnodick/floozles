    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Controls

if vspeed &lt; 3
{
    speedOffset = 2;
}
else
{
    speedOffset = 0;
}

// If the player is not pressing any buttons and they have ground beneath their feet, then they stop moving
if place_meeting(x, y + vspeed + speedOffset, oWall)
{
    
    y = other.y;
    vspeed = 0;
    hspeed = 0;
    if jumpCount == 1
    {
        audio_play_sound(sndLand, 10, false);
        jumpCount = 0;
    }
    
    sprite_index = sPlayerPerson;
    image_speed = 0.1;


    // Jump
        // If the pleyer presses Up and has ground under their feet they start moving upwards
    if keyboard_check_pressed(vk_up)
    {
        vspeed = -18;
        jumpCount += 1;
        audio_play_sound(sndJump5, 10, false);
    
        if ( keyboard_check(vk_left) and !place_meeting(x - speedUp, y, oWall) )
            hspeed = -speedJump;
        else if ( keyboard_check(vk_right) and !place_meeting(x + speedUp, y, oWall) )
            hspeed = speedJump;
    }
    
    // Move left
        // If the player is pressing Left and has ground under their feet then they do not fall
    else if keyboard_check(vk_left)
    {
        // If there is not a wall to the left of the player, move left
        if place_meeting(x - speedUp, y, oWall)
        {
            x = other.x + 1;
            hspeed = 0;
            
            if bumpDelay &gt; 0
            {
                bumpDelay -= 1;
            }
            else
            {
                audio_play_sound(sndBump, 10, false);
                bumpDelay = bumpDelayMax;
            }
        }
        else
        {
            hspeed = -speedUp;
            // Change the animation and speed it up
            sprite_index = sPlayerPersonWalkLeft;
            image_speed = imageFast; 
        }
    }
    
    // Move Right
        // If the player is pressing Right and has ground under their feet then they do not fall
    else if keyboard_check(vk_right)
    {
        // If there is not a wall to the right of the player, move right
        if place_meeting(x + speedUp, y, oWall)
        {
            x = other.x - 1;
            hspeed = 0;
            
            if bumpDelay &gt; 0
            {
                bumpDelay -= 1;
            }
            else
            {
                audio_play_sound(sndBump, 10, false);
                bumpDelay = bumpDelayMax;
            }
        }
        else
        {
            hspeed = speedUp;
            // Change the animation and speed it up
            sprite_index = sPlayerPersonWalkRight;
            image_speed = imageFast;
        }
    }

}

// If the player does not have any ground under their feet then they start falling
else if (vspeed &lt; 16 and y != oWall.y)
{
    vspeed += 2;
    jumpCount = 1;
    
    // Change the animation and speed it up
    sprite_index = sPlayerPersonWalkUp;
    image_speed = imageFast;
    

    if ( place_meeting(x - speedUp, y, oWall ) )
    {
        x = other.x + 1;
        hspeed = 0;
    }
    if ( place_meeting(x + speedUp, y, oWall ) )
    {
        x = other.x - 1;
        hspeed = 0;
    }

}

/*
if keyboard_check_pressed(vk_up) speed = 2;
if keyboard_check(vk_left) direction += 5;
if keyboard_check(vk_right) direction -= 5;
if keyboard_check_released(vk_up) speed = 0;
*/

// Shoot
if keyboard_check_pressed(ord('X'))
{
    if (firingDelay &gt; 0)
    {
        firingDelay -= 1;
    }
    else
    {
        instance_create (x + 11, y + 3, oPlayerShot);
        firingDelay = firingDelayMax;
    }
}
else
{
    firingDelay = 0;
}
</string>
          </argument>
