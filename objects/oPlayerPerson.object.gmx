            <string>/// Attributes

vspeed = 2;
image_speed = 0.1;

firingDelay = 0;
firingDelayMax = 10;
bumpDelay = 0;
bumpDelayMax = 4;

speedUp = 6;
speedJump = 8;
imageFast = 0.3;

jumpCount = 0;
</string>
